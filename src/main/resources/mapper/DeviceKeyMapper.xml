<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allong.centerstation.mapper.DeviceKeyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.allong.centerstation.domain.DeviceKey">
        <id column="id" property="id"/>
        <result column="code" property="code"/>
        <result column="device_id" property="deviceId"/>
        <result column="name" property="name"/>
        <result column="key_color" property="keyColor"/>
        <result column="key_size" property="keySize"/>
        <result column="min" property="min"/>
        <result column="max" property="max"/>
        <result column="position" property="position"/>
        <result column="wave" property="wave"/>
        <result column="scale" property="scale"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
       device_id, id, code, name, key_color, key_size, min, max, position, wave, scale
    </sql>

</mapper>
